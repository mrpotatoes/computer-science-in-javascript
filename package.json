{
  "name": "computer-science-in-javascript",
  "version": "1.0.0",
  "description": "Data structures in javascript",
  "main": "index.js",
  "homepage": "https://github.com/mrpotatoes/computer-science-in-javascript",
  "author": "Ben Oitvallon",
  "contributors": [
    "Andric LibreSinn"
  ],
  "license": "MIT",
  "scripts": {
    "start": "npm run test",
    "generate-hash": "babel-node src/hash-table/generate-hash-fixture.js",
    "test": "mocha --require babel-polyfill --recursive ./tests --compilers js:babel-register --watch",
    "test:bit-shifting": "mocha --require babel-polyfill ./src/bit-shifting/**/spec.*.js --compilers js:babel-register --watch",
    "test:compilers": "mocha --require babel-polyfill --recursive ./src/compilers --compilers js:babel-register --watch",
    "test:cryptography": "mocha --require babel-polyfill --recursive ./src/cryptography --compilers js:babel-register --watch",
    "test:data-structures": "mocha --require babel-polyfill --recursive ./src/data-structures --compilers js:babel-register --watch",
    "test:data-structures:graph": "mocha --require babel-polyfill --recursive ./src/data-structures/graph --compilers js:babel-register --watch",
    "test:design-patterns": "mocha --require babel-polyfill --recursive ./src/design-patterns --compilers js:babel-register --watch",
    "test:design-patterns:oop": "mocha --require babel-polyfill --recursive ./src/design-patterns/oop/behavioral/ --compilers js:babel-register --watch",
    "test:design-patterns:functional": "mocha --require babel-polyfill --recursive ./src/design-patterns/functional --compilers js:babel-register --watch",
    "test:design-patterns:react": "mocha --require babel-polyfill --recursive ./src/design-patterns/react --compilers js:babel-register --watch",
    "test:essential-javascript": "mocha --require babel-polyfill --recursive ./src/essential-javascript --compilers js:babel-register --watch",
    "test:interview-questions": "mocha --require babel-polyfill --recursive ./src/interview-questions --compilers js:babel-register --watch",
    "test:javascript": "mocha --require babel-polyfill --recursive ./src/javascript --compilers js:babel-register --watch",
    "test:math": "mocha --require babel-polyfill --recursive ./src/math --compilers js:babel-register --watch",
    "test:operating-systems": "mocha --require babel-polyfill --recursive ./src/operating-systems --compilers js:babel-register --watch",
    "test:recursion": "mocha --require babel-polyfill --recursive ./src/recursion/**/spec.*.js --compilers js:babel-register --watch --reporter mocha-clearscreen-reporter",
    "test:regex": "mocha --require babel-polyfill --recursive ./src/regex --compilers js:babel-register --watch",
    "test:sorting": "mocha --require babel-polyfill ./src/sorting/**/spec.*.js --compilers js:babel-register --watch"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-namespace": "^0.2.2",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.0",
    "babel-watch": "^2.0.6",
    "chai-spies": "^1.0.0",
    "mocha-clearscreen-reporter": "^1.0.1",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "mocha": "^3.3.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      [
        "namespace",
        {
          "disableSync": false,
          "sources": [
            "src"
          ],
          "includes": [
            "foo"
          ],
          "excludes": [
            "node_modules"
          ],
          "namespaces": {
            "tests": "./tests",
            "sorting": "./sorting",
            "data-structures": "./data-structures"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "extends": "airbnb",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "no-case-declarations": "off",
      "no-bitwise": "off",
      "no-plusplus": "off",
      "semi": [
        "error",
        "never"
      ],
      "arrow-parens": [
        "error",
        "always"
      ],
      "arrow-body-style": [
        "error",
        "as-needed"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/prefer-default-export": 0,
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/href-no-hash": 2,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "newline-per-chained-call": 0,
      "no-confusing-arrow": 0,
      "no-console": 1,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "class-methods-use-this": 0,
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "always",
          "named": "always"
        }
      ],
      "max-len": [
        "warn",
        {
          "code": 90,
          "ignoreComments": false,
          "ignoreUrls": true
        }
      ]
    }
  }
}
